@Given("set Tokenized header with filters") public void set_Tokenized_header_with_filters() {     String[] Filter = {"genre", "forrent"};          for (String Filter_Type : Filter) {         HashMap<String, String> query = new HashMap<>();         HashMap<String, String> headers = new HashMap<>();         if(Filter_Type=="genre"){             String Filter_value = "Action;Horror" ;         }         if(Filter_Type=="forrent"){             String Filter_value ="true";         }         return Filter_value;              headers.put("authorization", GlobalCache.get("authtoken").toString());     headers.put("accountid", GlobalCache.get("accountIdHash").toString());     headers.put("dt", GlobalCache.get("dt").toString());     headers.put("x-transaction-id", UUID.randomUUID().toString());     headers.put("portalId", GlobalCache.get("prt").toString());     headers.put("pt", GlobalCache.get("pt").toString());     headers.put("av", "6.0");     headers.put("rv", "6.0");     headers.put("Content-Type", "application/json");     headers.put("did", GlobalCache.get("deviceIdHash").toString());     headers.put(Filter_Type,Filter_value);         GlobalCache.setScenarioValue("Filter",Filter_Type);          commonSteps.apiInfo.setTokenizedHeader(headers);     commonSteps.api_call_is_made_for("Filmography");
