public void validateFilterResponse() {     Response response = (Response) GlobalCache.get("API_RESPONSE");      String filterType = GlobalCache.get("Filter").toString();      List<Map<String, Object>> componentsInfo = response.jsonPath().get("componentsInfo.content");     Map<String,Map<String, Object>> componentMap = new HashMap<>();           for (Map<String, Object> component : componentsInfo) {         if (component.containsKey("data")) {             String data= component.get("data").toString();              // Perform validation based on the filter type             switch (filterType) {                 case "genre":                     validateGenreFilter(data);                     break;                 // Add cases for other filter types as needed                 default:                     // Handle other filter types if necessary             }         }     } }
